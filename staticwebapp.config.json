{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/profile.html",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad",
      "statusCode": 302
    }
  }
}
